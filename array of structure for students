// Online C compiler to run C program online
#include <stdio.h>
struct student        
{
    char name[20];
     int roll;
     int age;
    
}s[3] ;
main( )
{
    int i;
    for(i=0;i<3;i++)
    {
        printf("enter name, roll no,age of student %d: " , i+1);
        scanf("%s %d %d" , s[i]. name, &s[i]. roll ,&s[i].age);
        printf("\n");
    }
    for(i=0;i<3;i++)
    {
        printf(" name , roll and age of student %d is:%s %d %d" ,i+1,s[i].name,s[i].roll,s[i].age  );
        printf("\n");
    }
    
    
        
       
     
    
}
     
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
   
