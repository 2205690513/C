// to find enter charcter is vowel or constant
#include <stdio.h>
main( )
{
    char ch;
    printf(" enter char ");
    scanf("%c" , &ch);
    if(ch== 'a'||ch== 'e'||ch== 'i'||ch== 'o'||ch== 'u'||ch== 'A'||ch== 'E'||ch== 'I'||ch== 'O'||ch== 'U')
    {
        printf(" char is vowel");
    }
    else
    {
        printf(" char is constant");
    }  
}  
    
    
